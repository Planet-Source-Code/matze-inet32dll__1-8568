\plain\f1\fs20\cf0\b Parameter\par
{\pard\trowd\trgaph57\trleft0\clbrdrt\brdrs\clbrdrb\brdrs\clbrdrl\brdrs\clbrdrr\brdrs\brdrw0\brdrcf0\brdrdot\cellx2261\clbrdrt\brdrs\clbrdrb\brdrs\clbrdrl\brdrs\clbrdrr\brdrs\brdrw0\brdrcf0\brdrdot\cellx8480\intbl \sb0\sl\sa0 \plain\f2\fs20\cf0 Variable\cell
\intbl \sb0\sl\sa0 \plain\f2\fs20\cf0 Erkl\'e4rung zur Variable.\cell\row
}\par
