\par
\plain\f1\fs20\cf0\b Parameter\par
{\pard\trowd\trgaph57\trleft0\clbrdrt\brdrs\clbrdrb\brdrs\clbrdrl\brdrs\clbrdrr\brdrs\brdrw0\brdrcf0\brdrdot\cellx2252\clbrdrt\brdrs\clbrdrb\brdrs\clbrdrl\brdrs\clbrdrr\brdrs\brdrw0\brdrcf0\brdrdot\cellx8446\intbl \sb0\sl\sa0 \plain\f2\fs20\cf0 Number\cell
\intbl \sb0\sl\sa0 \plain\f2\fs20\cf0 Eine Zahl die den Fehlercode enth\'e4lt.\cell\row
\intbl \sb0\sl\sa0 \plain\f2\fs20\cf0 Reason\cell
\intbl \sb0\sl\sa0 \plain\f2\fs20\cf0 Ein Text der den Grund des Fehler enth\'e4lt.\cell\row
}\page \keepn \plain\f0\fs20\cf0 {\cs16\super #{\*\footnote \pard\plain \s15\widctlpar \f4\fs20 {\cs16\super #} Muster}}\plain\f0\fs20\cf0 {\cs16\super ${\*\footnote \pard\plain \s15\widctlpar \f4\fs20 {\cs16\super $} Muster}}\plain\f1\fs28\cf0\b <Klasse>.<Element> Arial Black, 14Pkt. Fett\par
\plain\f2\fs20\cf0 Betrifft <Sprungziel <Klasse> Courier 10Pkt. Normal>\par\pard
\par
\plain\f1\fs20\cf0\b Deklaration  (Arial Black 10Pkt. Fett)\par
\plain\f2\fs20\cf0\i Objekt\plain\f2\fs20\cf0 .<Element>  (Bei Methoden oder Ereignissen) (Courier New 10Pkt. Normal)\par
\plain\f2\fs20\cf0 Objekt_<Ereignis>  (Bei Ereignissen) (Courier New 10Pkt. Normal)\par
\par
\plain\f1\fs20\cf0\b Beschreibung\par
